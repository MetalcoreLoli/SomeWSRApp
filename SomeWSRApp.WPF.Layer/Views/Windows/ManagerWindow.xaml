<Window x:Class="SomeWSRApp.WPF.Layer.Views.ManagerWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:SomeWSRApp.WPF.Layer.Views"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        Name="ManagerWin"
        Title="ManagerWindow" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="185"/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid>
            <StackPanel>
                <Button
                    Style="{StaticResource MainStyleWarningButton}"
                    Content="Sales"/>
            </StackPanel>
        </Grid> 
        <Grid Grid.Column="1">
            <Frame x:Name = "ManagerFrame"
                   Source="../Pages/ManagerSalesPage.xaml"></Frame>
        </Grid> 
          
    </Grid>
</Window>
